{
	"name": "@hydroperx/typedoc-java18-theme",
	"description": "TypeDoc Java SE 18 Theme",
	"version": "0.1.0",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"repository": "https://github.com/hydroperx/typedoc-java18-theme",
	"author": "Matheus Dias de Souza <hydroperfox@gmail.com>",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"typedoc": "~0.28.0"
	},
	"devDependencies": {
		"@eslint/js": "9.33.0",
		"@types/node": "^18.19.123",
		"eslint": "9.33.0",
		"eslint-config-prettier": "10.1.8",
		"prettier": "3.6.2",
		"typedoc": "~0.28.10",
		"typescript": "5.9.2",
		"typescript-eslint": "8.40.0"
	},
	"scripts": {
		"prepublishOnly": "npm run build:lib",
		"watch": "tsc --watch",
		"build": "npm run build:lib",
		"build:lib": "tsc",
		"build:docs": "typedoc src --plugin ./dist/index.js",
		"lint": "npm run lint:format && npm run lint:code",
		"lint:code": "eslint .",
		"lint:format": "prettier --check ."
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"keywords": [
		"typedoc-theme",
		"typedoc-javadoc-theme"
	]
}
